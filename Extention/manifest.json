{
	"manifest_version": 2,
	"name": "Bright YouTube",
	"description": "This extension automaticaly agusts the brightness of YouTube videos",
	"version": "0.3",
	
	"permissions": [
		"https://www.youtube.com/watch?v=*"
	],
	"content_scripts": [{
		"matches": [
			"https://www.youtube.com/watch?v=*"
		],
		"js": ["controll.js"],
		"css": ["temp.css"],
        "all_frames": true
	}]
}
