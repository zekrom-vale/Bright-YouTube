{
	"manifest_version": 2,
	"name": "Bright YouTube",
	"description": "This extension automaticaly ajusts the brightness of YouTube videos",
	"version": "0.4",
	
	"permissions": [
		"https://www.youtube.com/watch?v=*",
		"https://www.youtube.com/channel/*"
	],
	"optional_permissions": [
		"https://www.youtube.com/channel/*"
	],
	"content_scripts": [{
		"matches": [
			"https://www.youtube.com/watch?v=*",
			"https://www.youtube.com/channel/*"
		],
		"js": ["controll.js"],
		"css": ["temp.css"],
        "all_frames": true
	}]
}
