{
	"manifest_version": 2,
	"name": "Bright YouTube",
	"description": "This extension automaticaly agusts the brightness of YouTube vedieos",
	"version": "0.2",
	
	"permissions": [
		"*://www.youtube.com/"
	],
	"content_scripts": [{
		"matches": [
			"*://www.youtube.com/watch?v=*"
		],
		"js": ["controll.js"],
		"css": ["temp.css"],
        "all_frames": true
	}]
}
