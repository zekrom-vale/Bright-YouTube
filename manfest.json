{
	"manifest_version": 2,
	"name": "Bright YouTube",
	"description": "This extension automaticaly agusts the brightness of YouTube vedieos",
	"version": "0.1",
	"browser_action":{
		"default_icon": "icon.png",
        "default_popup": "UKNOWN.html"
	},
	"permissions": [
		"https://youtube.com/watch?v=*"
	"contentSettings"
	],
	"content_scripts": [{
		"matches": [
			"https://youtube.com/watch?v=*"
		],
		"js": ["controll.js"],
        "all_frames": true
	}],
    "offline_enabled": true
}
